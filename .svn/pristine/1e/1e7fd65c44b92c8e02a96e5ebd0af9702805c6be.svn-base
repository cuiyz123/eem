package com.metarnet.eomeem.utils;

/**
 * Created by Administrator on 2016/6/22.
 */
public class EemConstants {
    private String relativePath;
    public static String RELATIVE_PATH;

    private String gatherDataPath;
    public static String GATHER_DATA_PATH;

    public final static String SPLIT_STR = "v";
    private String evaluationFileAbsolutePath;
    public static String EVALUATION_FILE_ABSOLUTE_PATH;
    private Boolean reportTimeLock;
    public static Boolean REPORT_TIME_LOCK;

    private Boolean autoReportLock;
    public static Boolean AUTO_REPORT_LOCK;
    //生成数据临时目录
    public static String attachTempPath;

    private String orderDataPath;
    public static String ORDER_DATA_PATH;

    public String getRelativePath() {
        return relativePath;
    }

    public void setRelativePath(String relativePath) {
        this.relativePath = relativePath;
        this.RELATIVE_PATH = relativePath;
    }

    public String getGatherDataPath() {
        return gatherDataPath;
    }

    public void setGatherDataPath(String gatherDataPath) {
        this.gatherDataPath = gatherDataPath;
        this.GATHER_DATA_PATH = gatherDataPath;
    }

    public String getEvaluationFileAbsolutePath() {
        return evaluationFileAbsolutePath;
    }

    public void setEvaluationFileAbsolutePath(String evaluationFileAbsolutePath) {
        this.evaluationFileAbsolutePath = evaluationFileAbsolutePath;
        this.EVALUATION_FILE_ABSOLUTE_PATH = evaluationFileAbsolutePath;
    }

    public Boolean getReportTimeLock() {
        return reportTimeLock;
    }

    public void setReportTimeLock(Boolean reportTimeLock) {
        this.reportTimeLock = reportTimeLock;
        this.REPORT_TIME_LOCK = reportTimeLock;
    }

    public String getOrderDataPath() {
        return orderDataPath;
    }

    public void setOrderDataPath(String orderDataPath) {
        this.orderDataPath = orderDataPath;
        this.ORDER_DATA_PATH = orderDataPath;
    }

    public String getAttachTempPath() {
        return attachTempPath;
    }

    public void setAttachTempPath(String attachTempPath) {
        EemConstants.attachTempPath = attachTempPath;
    }

    public Boolean getAutoReportLock() {
        return autoReportLock;
    }

    public void setAutoReportLock(Boolean autoReportLock) {
        this.autoReportLock = autoReportLock;
        this.AUTO_REPORT_LOCK =autoReportLock;
    }
}
